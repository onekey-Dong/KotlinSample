<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.onekey.kotlinsample.MainActivity">

    <android.support.v7.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <android.support.v7.widget.AppCompatButton
            android:id="@+id/btnToKotlin"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onBtnClick"
            android:text="跳转kotlin页面" />

        <android.support.v7.widget.AppCompatButton
            android:id="@+id/btnToOne"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onBtnClick"
            android:text="跳转kotlin class页面" />

        <android.support.v7.widget.AppCompatButton
            android:id="@+id/btnToTwo"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onBtnClick"
            android:text="跳转类属性页面" />

        <android.support.v7.widget.AppCompatButton
            android:id="@+id/btnToThree"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onBtnClick"
            android:text="跳转kotlin页面" />

        <android.support.v7.widget.AppCompatButton
            android:id="@+id/btnToFour"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onBtnClick"
            android:text="跳转kotlin页面" />

        <android.support.v7.widget.AppCompatButton
            android:id="@+id/btnToFive"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onBtnClick"
            android:text="跳转kotlin页面" />
    </android.support.v7.widget.LinearLayoutCompat>
</android.support.v4.widget.NestedScrollView>
